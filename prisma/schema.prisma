// Minimal Prisma schema stub
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  createdAt DateTime @default(now())
}

model Job {
  id        String   @id
  kind      String
  status    String
  createdAt DateTime @default(now())
}


